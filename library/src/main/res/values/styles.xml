<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Keyboard-->
    <dimen name="size_text_keyboard">@dimen/_54sdp</dimen>
    <dimen name="size_text_keyboard_radius">@dimen/_27sdp</dimen>

    <style name="TextKeyboard" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/bg_circle_stroke</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

</resources>
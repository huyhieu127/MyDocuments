<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UButtonView">
        <attr name="uButtonName" format="string" />
        <attr name="uBackground" format="color" />
        <attr name="uElevation" format="dimension" />
        <attr name="uRadius" format="dimension" />
        <attr name="uIcon" format="reference" />
        <attr name="uIconLoading" format="reference" />
        <attr name="uEffectClick" format="enum">
            <enum name="alpha" value="0" />
            <enum name="ripple" value="1" />
        </attr>
        <attr name="uEnabled" format="boolean" />
    </declare-styleable>
</resources>